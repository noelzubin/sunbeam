{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "sunbeam",
      "type": "go",
      "request": "launch",
      "console": "integratedTerminal",
      "program": "${workspaceFolder}",
      "env": {
        "SUNBEAM_EXTENSIONS_DIR": "${workspaceFolder}/extensions",
      }
    },
    {
      "name": "sunbeam serve",
      "type": "go",
      "request": "launch",
      "program": "${workspaceFolder}",
      "args": [
        "serve"
      ],
      "env": {
        "SUNBEAM_EXTENSIONS_DIR": "${workspaceFolder}/extensions",
      }
    },
  ],
}
